{
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
			{
					"type": "pwa-node",
					"request": "launch",
					"name": "Transpiler: Debug Current Vite Test File",
					"autoAttachChildProcesses": true,
					"skipFiles": ["<node_internals>/**", "**/node_modules/**"],
					"program": "${workspaceRoot}/svws-transpile/npm/node_modules/vitest/vitest.mjs",
					"args": ["--config", "${workspaceRoot}/svws-transpile/npm/vite.config.ts","run", "${relativeFile}"],
					"smartStep": true,
					"console": "integratedTerminal"
			}
	]
}